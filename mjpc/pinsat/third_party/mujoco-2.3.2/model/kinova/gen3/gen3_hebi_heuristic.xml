<mujoco model="iiwa_workcell">
    <compiler angle="radian"/>
    <size nconmax="100" njmax="500" nstack="1270940"/>
    <asset>
        <!-- Gen3 Arm Meshes -->
        <mesh name="Base_Link" file="meshes/Base_Link.stl"/>
        <mesh name="Shoulder_Link" file="meshes/Shoulder_Link.stl"/>
        <mesh name="HalfArm1_Link" file="meshes/HalfArm1_Link.stl"/>
        <mesh name="HalfArm2_Link" file="meshes/HalfArm2_Link.stl"/>
        <mesh name="ForeArm_Link" file="meshes/ForeArm_Link.stl"/>
        <mesh name="SphericalWrist1_Link" file="meshes/SphericalWrist1_Link.stl"/>
        <mesh name="SphericalWrist2_Link" file="meshes/SphericalWrist2_Link.stl"/>
        <mesh name="Bracelet_Link" file="meshes/Bracelet_Link.stl"/>
        <mesh name="Hebi_Mount_Link" file="meshes/hebi_mount.stl" scale="0.001 0.001 0.001"/>
        <mesh name="Hebi_Proximal_Link" file="meshes/hebi_proximal.stl" scale="0.001 0.001 0.001"/>
        <mesh name="Hebi_Distal_Link" file="meshes/hebi_distal.stl" scale="0.001 0.001 0.001"/>

        <material name="BeigeMaterial" specular="0" shininess="0" reflectance="0" rgba="0.26 0.16 0.16796875 1" emission="0" />
        <texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" width="512" height="512" />
        <material name="MatPlane" reflectance="0" texture="texplane" texrepeat="1 1" texuniform="true" />
        <material name="MatGnd" specular=".3" shininess=".3" reflectance="0.3" rgba=".5 .55 .5 1"/>
    </asset>

    <custom>
        <numeric name="env_size" size="6" data="-1.5 -1.5 0 1.5 1.5 2.5"/>
    </custom>

    <worldbody>
        <!-- Lights -->
        <light name="light0" diffuse=".5 .5 .5" pos="1.5 1.5 2"   dir="-1 -1 -.5" castshadow="false" />
        <light name="light1" diffuse=".5 .5 .5" pos="-1.5 1.5 2"  dir="1 -1 -.5" castshadow="false" />
        <light name="light2" diffuse=".5 .5 .5" pos="1.5 -1.5 2"  dir="-1 1 -.5" castshadow="false" />
        <light name="light3" diffuse=".5 .5 .5" pos="-1.5 -1.5 2" dir="1 1 -.5" castshadow="false" />

        <!-- obstacles -->
        <body name="obstacles" pos="0 0 0">
            <include file="obstacles.xml"/>
        </body>

        <!-- env limits represented as with an outer dome -->
        <body name="outer_dome" pos="0.0 0.0 0.0" quat="1 0 0 0">
            <geom name="w1" size="0.05 1.5 1.25" pos="1.5 0.0 1.25" type="box" rgba="0.7 0.7 0.0 0.3" contype="3" conaffinity="3"/>
            <geom name="w2" size="1.5 0.05 1.25" pos="0.0 1.5 1.25" type="box" rgba="0.7 0.7 0.0 0.3" contype="3" conaffinity="3"/>
            <geom name="w3" size="0.05 1.5 1.25" pos="-1.5 0.0 1.25" type="box" rgba="0.7 0.7 0.0 0.3" contype="3" conaffinity="3"/>
            <geom name="w4" size="1.5 0.05 1.25" pos="0.0 -1.5 1.25" type="box" rgba="0.7 0.7 0.0 0.3" contype="3" conaffinity="3"/>
            <geom name="w5" size="1.5 1.5 0.05" pos="0 0 0" type="box" rgba="0.7 0.7 0.0 0.3" contype="3" conaffinity="3"/>
            <geom name="w6" size="1.5 1.5 0.05" pos="0 0 2.5" type="box" rgba="0.7 0.7 0.0 0.3" contype="3" conaffinity="3"/>
        </body>

        <!-- 3D  -->
        <body name="b1" pos="0.55 -0.7 0.75" quat="1 0 0 0">
            <inertial pos="0 0 0" mass=".1" diaginertia="1e-4 1e-4 1e-4"/>
            <joint name="b1" type="free" limited="false" />
            <geom name="b1" type="sphere" size="0.02" rgba="1 1 0 1" contype="1" conaffinity="1"/>
        </body>

    </worldbody>

</mujoco>